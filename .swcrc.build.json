{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript"
    },
    "target": "es2020"
  },
  "module": {
    "type": "commonjs"
  },
  "exclude": [
    ".*__fixtures__.*",
    ".*__mocks__.*",
    ".*__snapshots__.*",
    ".*__tests?__.*",
    ".*test-utils.*",
    ".*\\.test\\.?.*\\.tsx?",
    ".*\\.d\\.tsx?",
    ".*eval-worker\\.js",
    ".*browserify\\.js"
  ],
  "sourceMaps": true
}
