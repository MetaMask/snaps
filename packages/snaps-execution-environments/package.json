{
  "name": "@metamask/snaps-execution-environments",
  "version": "0.32.2",
  "description": "Snap sandbox environments for executing SES javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-monorepo.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "rimraf coverage && jest && yarn test:browser && yarn posttest",
    "posttest": "ts-node scripts/coverage.ts && rimraf coverage/jest coverage/wdio",
    "test:browser": "wdio run wdio.config.ts",
    "test:ci": "yarn test",
    "test:watch": "jest --watch",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "clean": "rimraf '*.tsbuildinfo' 'dist/*' 'src/__GENERATED__/' 'coverage/*' '__test__/*'",
    "build:clean": "yarn clean && yarn build",
    "build:post-tsc": "yarn build:lavamoat",
    "build:tsc": "tsc --project tsconfig.build.json",
    "build": "yarn build:tsc && yarn build:post-tsc",
    "build:test": "yarn build:lavamoat",
    "build:lavamoat": "lavamoat scripts/build.js --policy lavamoat/build-system/policy.json  --policyOverride lavamoat/build-system/policy-override.json",
    "build:lavamoat:policy": "yarn build:lavamoat --writeAutoPolicy && node scripts/build.js --writeAutoPolicy",
    "auto-changelog-init": "auto-changelog init",
    "prepare-manifest:preview": "../../scripts/prepare-preview-manifest.sh",
    "publish:preview": "yarn npm publish --tag preview"
  },
  "dependencies": {
    "@metamask/object-multiplex": "^1.2.0",
    "@metamask/post-message-stream": "^6.1.1",
    "@metamask/providers": "^10.2.0",
    "@metamask/rpc-methods": "^0.32.2",
    "@metamask/snaps-utils": "^0.32.2",
    "@metamask/utils": "^5.0.0",
    "eth-rpc-errors": "^4.0.3",
    "json-rpc-engine": "^6.1.0",
    "pump": "^3.0.0",
    "ses": "^0.18.1",
    "stream-browserify": "^3.0.0",
    "superstruct": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@browserify/uglifyify": "^6.0.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@lavamoat/lavapack": "^3.3.0",
    "@lavamoat/lavatube": "^0.0.1",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@wdio/browser-runner": "^8.3.10",
    "@wdio/cli": "^8.3.10",
    "@wdio/globals": "^8.3.10",
    "@wdio/mocha-framework": "^8.3.0",
    "@wdio/spec-reporter": "^8.3.0",
    "@wdio/static-server-service": "^8.3.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "common-shakeify": "^1.1.2",
    "deepmerge": "^4.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-webdriverio": "^4.1.2",
    "istanbul-lib-coverage": "^3.2.0",
    "istanbul-lib-report": "^3.0.0",
    "istanbul-reports": "^3.1.5",
    "jest": "^29.0.2",
    "jest-fetch-mock": "^3.0.3",
    "lavamoat": "^6.4.0",
    "lavamoat-browserify": "^15.5.0",
    "memfs": "^3.4.13",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "process": "^0.11.10",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-geckodriver-service": "^4.1.0",
    "webdriverio": "^8.3.10",
    "yargs": "^17.7.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
