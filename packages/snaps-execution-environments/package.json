{
  "name": "@metamask/snaps-execution-environments",
  "version": "0.30.0",
  "description": "Snap sandbox environments for executing SES javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-monorepo.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest && yarn posttest",
    "posttest": "jest-it-up --margin 0.25",
    "test:browser": "wdio run wdio.config.ts",
    "test:ci": "yarn test",
    "test:watch": "jest --watch",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "clean": "rimraf '*.tsbuildinfo' 'dist/*' 'src/__GENERATED__/' 'coverage/*' '__test__/*'",
    "build:clean": "yarn clean && yarn build",
    "build:post-tsc": "webpack --mode none && yarn build:concat",
    "build:tsc": "tsc --project tsconfig.build.json",
    "build": "yarn build:tsc && yarn build:post-tsc",
    "build:test": "cross-env NODE_ENV=test webpack --mode none --config-name iframe-test --config-name node-process --config-name node-thread && yarn build:concat",
    "build:concat": "concat -o ./dist/webpack/node-process/bundle.js ./dist/webpack/node-process/lockdown.umd.min.js ./dist/webpack/node-process/bundle.js && concat -o ./dist/webpack/node-thread/bundle.js ./dist/webpack/node-thread/lockdown.umd.min.js ./dist/webpack/node-thread/bundle.js",
    "auto-changelog-init": "auto-changelog init",
    "publish:package": "../../scripts/publish-package.sh"
  },
  "dependencies": {
    "@metamask/object-multiplex": "^1.2.0",
    "@metamask/post-message-stream": "^6.1.0",
    "@metamask/providers": "^10.2.0",
    "@metamask/rpc-methods": "^0.30.0",
    "@metamask/snaps-utils": "^0.30.0",
    "@metamask/utils": "^3.4.1",
    "eth-rpc-errors": "^4.0.3",
    "json-rpc-engine": "^6.1.0",
    "pump": "^3.0.0",
    "ses": "^0.18.1",
    "stream-browserify": "^3.0.0",
    "superstruct": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-typescript": "^7.18.6",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@lavamoat/lavatube": "^0.0.1",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@wdio/browser-runner": "^8.3.10",
    "@wdio/cli": "^8.3.10",
    "@wdio/globals": "^8.3.10",
    "@wdio/mocha-framework": "^8.3.0",
    "@wdio/spec-reporter": "^8.3.0",
    "@wdio/static-server-service": "^8.3.0",
    "babel-loader": "^9.1.2",
    "buffer": "^6.0.3",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-webdriverio": "^4.1.2",
    "html-inline-script-webpack-plugin": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-it-up": "^2.1.0",
    "jsdom": "^19.0.0",
    "memfs": "^3.4.13",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-geckodriver-service": "^4.1.0",
    "webdriverio": "^8.3.10",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
