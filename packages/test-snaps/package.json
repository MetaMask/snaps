{
  "name": "@metamask/test-snaps",
  "version": "0.35.2-flask.1",
  "private": true,
  "description": "The test snaps website for MetaMask Snaps, used for end-to-end testing.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps.git"
  },
  "scripts": {
    "start": "gatsby develop --port 8080",
    "build": "gatsby build --prefix-paths",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf '.cache' 'public'",
    "lint:eslint": "eslint . --cache --ext js,jsx,ts,tsx",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' '**/*.html' --single-quote --ignore-path ../../.prettierignore --no-error-on-unmatched-pattern",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate"
  },
  "dependencies": {
    "@metamask/snaps-utils": "workspace:^",
    "@metamask/utils": "^6.0.1",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^1.9.5",
    "bootstrap": "^5.2.2",
    "gl-matrix": "^3.4.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "simplex-noise": "^4.0.0"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@metamask/providers": "^11.0.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gatsby": "^5.11.0",
    "gatsby-plugin-react-redux": "^1.1.0",
    "gatsby-plugin-sass": "^6.11.0",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
