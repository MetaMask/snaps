    "@metamask/snaps-ui": "workspace:^",
    "@metamask/snaps-utils": "workspace:^",
    "@metamask/utils": "^6.0.0",
    "@minoru/react-dnd-treeview": "^3.4.4",
    "@reduxjs/toolkit": "^1.9.5",
    "date-fns": "^2.30.0",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^10.12.8",
