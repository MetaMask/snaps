{
  "name": "@metamask/snaps-browserify-plugin",
  "version": "0.33.1-flask.1",
  "keywords": [
    "browserify-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-monorepo.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest && yarn posttest",
    "posttest": "jest-it-up",
    "test:ci": "yarn test",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path ../../.gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate",
    "build": "yarn build:cjs && yarn build:esm",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf coverage && rimraf dist && rimraf \"*.tsbuildinfo\"",
    "prepare-manifest:preview": "../../scripts/prepare-preview-manifest.sh",
    "publish:preview": "yarn npm publish --tag preview",
    "build:cjs": "swc --source-maps --config module.type=commonjs --out-dir dist/cjs src",
    "build:esm": "swc --source-maps --config module.type=es6 --out-dir dist/esm src"
  },
  "dependencies": {
    "@metamask/snaps-utils": "workspace:^",
    "convert-source-map": "^1.8.0"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.3.1",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.60",
    "@swc/jest": "^0.2.26",
    "@types/browserify": "^12.0.37",
    "@types/convert-source-map": "^1.5.2",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "browserify": "^17.0.0",
    "concat-stream": "^2.0.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.2",
    "jest-it-up": "^2.0.0",
    "memfs": "^3.4.13",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.2",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
