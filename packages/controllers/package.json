{
  "name": "@metamask/snap-controllers",
  "version": "0.10.0",
  "description": "Controllers for MetaMask Snaps.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-skunkworks.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test:prepare": "yarn mkdirp test/fixtures && ./scripts/generate-fixtures.sh",
    "test": "yarn test:prepare && jest",
    "test:ci": "yarn test:prepare && ./scripts/test-ci.sh",
    "build:pre-tsc": "yarn build:ajv && node scripts/transpileSchemaTypes.js",
    "build:post-tsc": "echo 'N/A'",
    "build:tsc": "tsc --project tsconfig.local.json",
    "ajv-compile": "ajv compile --strict=true --all-errors",
    "build:ajv": "./scripts/build-ajv.sh",
    "build": "yarn build:pre-tsc && yarn build:tsc",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist/*",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path ../../.gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate",
    "publish": "../../scripts/publish-package.sh"
  },
  "dependencies": {
    "@metamask/controllers": "^26.0.0",
    "@metamask/execution-environments": "^0.10.0",
    "@metamask/object-multiplex": "^1.1.0",
    "@metamask/obs-store": "^7.0.0",
    "@metamask/post-message-stream": "4.0.0",
    "@metamask/safe-event-emitter": "^2.0.0",
    "@types/deep-freeze-strict": "^1.1.0",
    "@types/semver": "^7.3.9",
    "ajv": "^8.8.2",
    "concat-stream": "^2.0.0",
    "cross-fetch": "^3.1.5",
    "deep-freeze-strict": "^1.1.1",
    "eth-rpc-errors": "^4.0.2",
    "fast-deep-equal": "^3.1.3",
    "gunzip-maybe": "^1.4.2",
    "immer": "^9.0.6",
    "json-rpc-engine": "^6.1.0",
    "json-rpc-middleware-stream": "^3.0.0",
    "nanoid": "^3.1.31",
    "pump": "^3.0.0",
    "readable-web-to-node-stream": "^3.0.2",
    "semver": "^7.3.5",
    "tar-stream": "^2.2.0"
  },
  "devDependencies": {
    "@jest-runner/electron": "^3.0.1",
    "@json-schema-tools/transpiler": "^1.10.2",
    "@metamask/snap-types": "^0.10.0",
    "@metamask/template-snap": "^0.7.0",
    "@types/concat-stream": "^1.6.1",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/jest": "^26.0.23",
    "@types/pump": "^1.1.0",
    "@types/readable-stream": "^2.3.9",
    "@types/semver": "^7.3.9",
    "@types/tar-stream": "^2.2.2",
    "ajv-cli": "^5.0.0",
    "electron": "^12.1.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "mkdirp": "^1.0.4",
    "ts-jest": "^26.5.6"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
