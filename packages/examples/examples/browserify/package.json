{
  "name": "browserify-snap",
  "version": "0.34.1-flask.1",
  "private": true,
  "description": "An example Snap built using TypeScript and Browserify",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps.git"
  },
  "license": "ISC",
  "main": "src/index.js",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "gulp build",
    "manifest": "mm-snap manifest --fix",
    "eval": "mm-snap eval -b dist/snap.js",
    "serve": "concurrently \"gulp watch\" \"mm-snap serve\"",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts,jsx,tsx",
    "lint:misc": "prettier --no-error-on-unmatched-pattern --loglevel warn \"**/*.json\" \"**/*.md\" \"**/*.html\" \"!CHANGELOG.md\" --ignore-path ../../../../.gitignore",
    "lint:ci": "yarn lint"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.12",
    "@babel/preset-typescript": "^7.20.12",
    "@metamask/eslint-config": "^11.0.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@metamask/snaps-browserify-plugin": "workspace:^",
    "@metamask/snaps-cli": "workspace:^",
    "@metamask/snaps-types": "workspace:^",
    "@types/browserify": "^12.0.37",
    "@types/gulp": "^4.0.9",
    "@types/node": "^17.0.36",
    "@types/vinyl-source-stream": "^0.0.30",
    "@types/watchify": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
