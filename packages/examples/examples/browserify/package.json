{
  "name": "browserify-snap",
  "version": "0.0.0",
  "private": true,
  "description": "An example Snap built using TypeScript and Browserify",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-skunkworks.git"
  },
  "license": "ISC",
  "main": "src/index.js",
  "files": [
    "dist/",
    "snap.manifest.json"
