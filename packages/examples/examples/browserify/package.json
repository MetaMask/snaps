    "@types/gulp": "^4.0.9",
    "@types/node": "^17.0.33",
    "@types/vinyl-source-stream": "^0.0.30",
    "@types/watchify": "^3.11.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-exec": "^5.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vinyl-source-stream": "^2.0.0",
