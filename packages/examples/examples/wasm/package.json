{
  "name": "wasm",
  "version": "0.30.0",
  "private": true,
  "description": "An example Snap that uses WebAssembly.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-monorepo.git"
  },
  "license": "ISC",
  "main": "src/index.js",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "scripts": {
    "prebuild": "yarn run build:wasm",
    "build": "yarn run prebuild && mm-snap build",
