    "concurrently": "^7.1.0",
    "hook-shell-script-webpack-plugin": "^0.1.4",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "engines": {
    "node": ">=12.11.0"
