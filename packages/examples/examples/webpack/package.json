    "rimraf": "^3.0.2",
    "typescript": "~4.8.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
