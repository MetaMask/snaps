{
  "name": "signer-core",
  "version": "0.34.0-flask.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps.git"
  },
  "scripts": {
    "build": "mm-snap build",
    "start": "mm-snap serve",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
