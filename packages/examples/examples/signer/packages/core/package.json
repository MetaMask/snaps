    "lint:ci": "yarn lint"
  },
  "dependencies": {
    "@metamask/utils": "^6.0.0",
    "async-mutex": "^0.4.0"
  },
  "devDependencies": {
    "@metamask/key-tree": "^7.0.0",
    "@metamask/snaps-cli": "workspace:^",
    "@metamask/snaps-types": "workspace:^"
  },
  "packageManager": "yarn@3.4.1"
}
