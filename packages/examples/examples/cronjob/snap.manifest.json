{
  "version": "0.32.2",
  "description": "A MetaMask Test Snap that demonstrates usage of the cronjob endowment.",
  "proposedName": "MetaMask Cronjob Snap Example",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-monorepo.git"
  },
  "source": {
    "shasum": "jazhnrSl1gpeawXBIdz2I2/lc+kmKc3tVsFUpypAhAE=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "cronjob-snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "fireCronjob"
          }
        }
      ]
    },
    "snap_dialog": {}
  },
  "manifestVersion": "0.1"
}
