{
  "version": "1.0.0",
  "description": "A MetaMask Test Snap that demonstrates usage of the cronjob endowment.",
  "proposedName": "MetaMask Cronjob Snap Example",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/test-snaps.git"
  },
  "source": {
    "shasum": "e2eneMYchQZaD4Cjb1tsQoP/yKUpxNpp3JW+zKXxR68=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "cronjob-snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "fireCronjob"
          }
        }
      ]
    },
    "snap_dialog": {}
  },
  "manifestVersion": "0.1"
}
