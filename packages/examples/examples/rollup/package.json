{
  "name": "rollup-snap",
  "version": "0.33.1-flask.1",
  "private": true,
  "description": "An example Snap built using TypeScript and Rollup",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-monorepo.git"
  },
  "license": "ISC",
  "main": "src/index.js",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "scripts": {
    "mm-snap": "ts-node --swc ../../../snaps-cli/src/main.ts",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "build": "ts-node --swc -r tsconfig-paths/register ../../../../node_modules/.bin/rollup --config",
    "serve": "concurrently \"rollup --config --watch\" \"yarn mm-snap serve\""
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-typescript": "^7.20.12",
    "@lavamoat/allow-scripts": "^2.3.1",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@metamask/snaps-cli": "workspace:^",
    "@metamask/snaps-rollup-plugin": "workspace:^",
    "@metamask/snaps-types": "workspace:^",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^4.1.2",
    "rollup": "^2.73.0",
    "tslib": "^2.4.0",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
